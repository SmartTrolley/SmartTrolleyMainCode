<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<VBox id="VBox" alignment="CENTER" fillWidth="false" prefHeight="600.0"
	prefWidth="600.0" spacing="0.0" styleClass="containerMain" xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/2.2" fx:controller="smarttrolleygui.AllShoppingListsScreenController">
	<children>
		<ImageView fitHeight="150.0" fitWidth="450.0" pickOnBounds="true"
			preserveRatio="true" smooth="true" x="0.0" y="0.0">
			<image>
				<Image url="@../img/companyLogoCropped.png" />
			</image>
			<VBox.margin>
				<Insets bottom="30.0" />
			</VBox.margin>
		</ImageView>
		<Line endX="575.0" startX="0.0" strokeLineCap="ROUND" />
		<Button fx:id="list1Button" mnemonicParsing="false" onAction="#loadShoppingList"
			prefHeight="49.0" prefWidth="380.0" styleClass="buttonLarge" text="04/02/14 - Food"
			textAlignment="LEFT" textFill="BLACK" underline="false" wrapText="false">
			<VBox.margin>
				<Insets top="70.0" />
			</VBox.margin>
		</Button>		
		<Button fx:id="goBackButton" mnemonicParsing="false" onAction="#loadStartScreen"
			prefHeight="113.0" prefWidth="380.0" styleClass="buttonLarge" text="Go back"
			textAlignment="LEFT" textFill="BLACK" underline="false" wrapText="true">
			<VBox.margin>
				<Insets top="60.0" />
			</VBox.margin>
		</Button>
		<HBox prefHeight="21.0" prefWidth="322.0">
			<children>
				<Label text="Create your shopping list online @ " />
				<Hyperlink text="www.smartrolley.org" />
			</children>
			<VBox.margin>
				<Insets top="50.0" />
			</VBox.margin>
		</HBox>
	</children>
	<stylesheets>
		<URL value="@../StyleSheet.css" />
	</stylesheets>
</VBox>
